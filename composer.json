{
    "name": "toraido/fatura",
    "type": "symfony-bundle",
    "description": "Generate invoices using a simple API",
    "license": "MIT",
    "keywords": ["invoice", "invocing", "invoice-generator", "invoice-pdf", "symfony-bundle"],
    "authors": [
        {
            "name": "azjezz",
            "email": "azjezz@protonmail.com"
        }
    ],
    "require": {
        "php": "~8.0.0 || ~8.1.0",
        "ext-intl": "*",
        "azjezz/psl": "^0.1",
        "moneyphp/money": "^3.3",
        "symfony/framework-bundle": "^5.2",
        "twig/twig": "^3.1",
        "twig/extra-bundle": "^3.1",
        "twig/cssinliner-extra": "^3.1",
        "knplabs/knp-snappy-bundle": "^1.8",
        "symfony/translation": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "dev-master",
        "vimeo/psalm": "^4.3",
        "psalm/plugin-phpunit": "^0.15.0"
    },
    "autoload": {
        "psr-4": {
            "Toraido\\Fatura\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toraido\\Fatura\\Test\\": "tests/"
        }
    }
}
